<config>

  <OStreamMessageSink stream="err" useAnsiColor="1" level="1"/>
  <LogFileMessageSink logFilePath="gramod.log" append="0"/>

  <ImportLibrary lib="libgmGraphics.so"/>

  <SdlWindow size="1920 1080">
    <SdlContext AS="context" useVideo="1"/>
    <TextureRenderer AS="renderer">
      <UvcTexture
          AS="texture"
          resolution="1920 1080"
          framerate="30"
          format="mjpeg"/>
    </TextureRenderer>
  </SdlWindow>

</config>
