<config>

  <OStreamMessageSink stream="err" useAnsiColor="1" level="1"/>
  <LogFileMessageSink logFilePath="gramod.log" append="0"/>

  <ImportLibrary lib="libgmGraphics.so"/>

  <SdlWindow
      size="640 640"
      GLMajor="3" GLMinor="3">
    <SdlContext
        AS="context"
        useVideo="1"/>
    <TextureRenderer AS="renderer">
      <UvcTexture
          AS="texture"
          resolution="640 480"
          framerate="30"
          format="mjpeg"/>
    </TextureRenderer>
  </SdlWindow>

</config>
