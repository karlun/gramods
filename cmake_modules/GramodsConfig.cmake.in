SET(GRAMODS_VERSION x.y.z)

@PACKAGE_INIT@

FOREACH(COMPONENT ${Gramods_FIND_COMPONENTS})
  IF (EXISTS ${CMAKE_CURRENT_LIST_DIR}/Gramods-${COMPONENT}-targets.cmake)
    INCLUDE(${CMAKE_CURRENT_LIST_DIR}/Gramods-${COMPONENT}-targets.cmake)
    IF (TARGET ${COMPONENT})
      SET (Gramods_${COMPONENT}_FOUND TRUE)
    ENDIF()
  ENDIF()
ENDFOREACH()

SET (Gramods_FOUND TRUE)
CHECK_REQUIRED_COMPONENTS(Gramods)
